  </entityFramework>
  <connectionStrings>
    <add name="MsSqlDbContext" providerName="System.Data.SqlClient" connectionString="Initial Catalog=SmartLibrary;User Id=sa;Password=123@abc;Data Source=192.168.100.200" />
